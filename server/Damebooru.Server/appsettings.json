{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=../../data/damebooru.db"
  },
  "Damebooru": {
    "Storage": {
      "DatabasePath": "../../data/damebooru.db",
      "ThumbnailPath": "../../data/thumbnails",
      "TempPath": "../../data/temp"
    },
    "Processing": {
      "RunScheduler": true,
      "MetadataParallelism": 2,
      "SimilarityParallelism": 6,
      "ThumbnailParallelism": 2
    },
    "Scanner": {
      "BatchSize": 100,
      "Parallelism": 2
    },
    "Ingestion": {
      "BatchSize": 100,
      "ChannelCapacity": 1000
    },
    "Auth": {
      "Enabled": true,
      "Username": "admin",
      "Password": "change-me"
    },
    "Logging": {
      "Db": {
        "Enabled": true,
        "MinimumLevel": "Warning",
        "BatchSize": 100,
        "FlushIntervalMs": 1000,
        "ChannelCapacity": 10000,
        "RetentionDays": 7,
        "MaxRows": 10000,
        "RetentionCheckIntervalMinutes": 15
      }
    }
  }
}
